{
    "name": "coronavirus-chile",
    "description": "Simple and user-friendly analysis of coronavirus cases by region and by comuna in Chile.",
    "scripts": {},
    "env": {
      "REACT_APP_ENV": {
        "value": "staging"
      },
      "REACT_APP_CHILE_DATA_JSON_URL": {
        "value": "https://raw.githubusercontent.com/javierlopeza/datos-coronavirus-chile/master/data/chile-minified.json"
      },
      "REACT_APP_CURRENT_NOTIFICATION_JSON_URL": {
        "value": "https://raw.githubusercontent.com/javierlopeza/datos-coronavirus-chile/master/notifications/current_notification.json"
      }
    },
    "formation": {
      "web": {
        "quantity": 1
      }
    },
    "addons": [],
    "buildpacks": [
      {
        "url": "https://buildpack-registry.s3.amazonaws.com/buildpacks/mars/create-react-app.tgz"
      }
    ],
    "stack": "heroku-18"
  }
